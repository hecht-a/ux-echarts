{
  "name": "hecht-a/ux-echarts",
  "type": "symfony-bundle",
  "description": "Echarts integration for Symfony",
  "keywords": ["symfony-ux"],
  "homepage": "https://github.com/hecht-a/ux-echarts",
  "license": "MIT",
  "authors": [
    {
      "name": "Axel HECHT",
      "email": "axel.hct@gmail.com",
      "homepage": "https://github.com/hecht-a"
    }
  ],
  "autoload": {
    "psr-4": {
      "HechtA\\UX\\ECharts\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HechtA\\UX\\ECharts\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3",
    "symfony/config": "^7.0",
    "symfony/dependency-injection": "^7.0",
    "symfony/http-kernel": "^7.0",
    "symfony/stimulus-bundle": "^2.9.1"
  },
  "require-dev": {
    "symfony/framework-bundle": "^7.0",
    "symfony/phpunit-bridge": "^8.0",
    "symfony/twig-bundle": "^7.0",
    "symfony/var-dumper": "^7.0",
    "phpunit/phpunit": "^12.5"
  },
  "conflict": {
    "symfony/flex": "<1.13"
  },
  "extra": {
    "thanks": {
      "name": "symfony/ux",
      "url": "https://github.com/symfony/ux"
    }
  }
}
